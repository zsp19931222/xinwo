<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    android:id="@+id/identity_authentication_include3Lin"
    android:orientation="vertical" >

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="35dp"
        android:background="@drawable/identity_test_bg3" />

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:padding="@dimen/marginTen"
        android:text="资料上传成功，预计将于两个工作日内审核完成！"
        android:textColor="@color/淡灰色"
        android:textSize="@dimen/小字体" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/三c" />

    <Button
        android:id="@+id/identity_authentication_confirmBtn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="26dp"
        android:layout_marginRight="26dp"
        android:layout_marginTop="50dp"
        android:background="@drawable/publicbtn_selector_bg"
        android:paddingBottom="10dp"
        android:paddingTop="10dp"
        android:text="确定"
        android:textColor="@color/white"
        android:textSize="14sp" />

</LinearLayout>